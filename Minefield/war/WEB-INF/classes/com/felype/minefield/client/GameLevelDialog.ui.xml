<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:DialogBox autoHide="true" modal="true" >
		<g:caption>
			<b>Choose a level:</b>
		</g:caption>
		<g:VerticalPanel horizontalAlignment="center" >
			<g:Button ui:field="buttonEasy" text="Easy" />
			<g:Button ui:field="buttonMedium" text="Medium" />
			<g:Button ui:field="buttonHard" text="Hard" />
		</g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 